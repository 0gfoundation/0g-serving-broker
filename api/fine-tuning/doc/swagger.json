{
    "swagger": "2.0",
    "info": {
        "description": "These APIs allows customers to interact with the 0G Compute Fine Tune Service",
        "title": "0G Serving Provider Broker API",
        "contact": {},
        "version": "0.2.0"
    },
    "host": "localhost:3080",
    "basePath": "/v1",
    "paths": {
        "/task": {
            "post": {
                "description": "This endpoint allows you to create fine tune task",
                "tags": [
                    "task"
                ],
                "operationId": "createTask",
                "parameters": [
                    {
                        "description": "body",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/schema.Task"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content - success without response body"
                    }
                }
            }
        },
        "/task/{id}": {
            "get": {
                "description": "This endpoint allows you to get task by name",
                "tags": [
                    "task"
                ],
                "operationId": "getTask",
                "parameters": [
                    {
                        "type": "string",
                        "description": "task ID",
                        "name": "taskID",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/schema.Task"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "schema.Task": {
            "type": "object",
            "required": [
                "command",
                "customerAddress",
                "datasetHash",
                "epochNumber",
                "fineTunedScriptHash",
                "preTrainedModelHash"
            ],
            "properties": {
                "command": {
                    "type": "string"
                },
                "createdAt": {
                    "type": "string",
                    "readOnly": true
                },
                "customerAddress": {
                    "type": "string"
                },
                "datasetHash": {
                    "type": "string"
                },
                "epochNumber": {
                    "type": "integer"
                },
                "fineTunedScriptHash": {
                    "type": "string"
                },
                "id": {
                    "type": "string",
                    "readOnly": true
                },
                "preTrainedModelHash": {
                    "type": "string"
                },
                "progress": {
                    "type": "integer",
                    "readOnly": true
                },
                "updatedAt": {
                    "type": "string",
                    "readOnly": true
                }
            }
        }
    }
}